{
    "editor.minimap.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.fontFamily": "Consolas,monospace",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.mouseWheelZoom": true,
    "peacock.color": "#d46ce2",
    "python.defaultInterpreterPath": "./.venv/bin/python",
    "python.terminal.activateEnvironment": false,
    "markdown.styles": [
        "code-notes-infograph/css/code-notes.css"
    ],
    "workbench.colorTheme": "Default Light Modern",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#357ffd",
        "activityBar.background": "#357ffd",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#9e023b",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#357ffd",
        "statusBar.background": "#035ffc",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#357ffd",
        "statusBarItem.remoteBackground": "#035ffc",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#035ffc",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#035ffc99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "gitdoc.commitValidationLevel": "none",
    "gitdoc.timeZone": "Europe/Moscow",
    "gitdoc.commitMessageFormat": "yyyy-LL-DD-HH-mm-ss",
    "peacock.remoteColor": "#035ffc",
    "gitdoc.enabled": true
}